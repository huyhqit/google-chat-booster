{
  "name": "Google chat booster",
  "description": "Add more utilities for google chat",
  "version": "1.0.0",
  "manifest_version": 3,
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {},
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*", "https://chat.google.com/*"],
      "run_at": "document_end",
      "js": ["contentScript.js"],
      "all_frames": true
    }
  ],
  "update_url": "https://clients2.google.com/service/update2/crx"
}
